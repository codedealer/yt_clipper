{
  "name": "typescript",
  "author": "exwm",
  "homepage": "https://openuserjs.org/scripts/elwm/yt_clipper",
  "version": "0.0.75",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/exwm/yt_clipper"
  },
  "description": "add markers to youtube videos and generate clipped webms online or offline",
  "main": "./src/yt_clipper.ts",
  "devDependencies": {
    "npm-run-all": "^4.1.5",
    "prettier": "1.17.1",
    "typescript": "3.3.x"
  },
  "scripts": {
    "pretty": "npx prettier \"**/*.{ts,js,json,md}\" --write",
    "build:ts": "npx tsc",
    "build:py": "pyinstaller ./src/yt_clipper.py -F --workpath ./dist/py/ --distpath ./dist/py/ --specpath ./dist/py/spec",
    "build:all": "run-s -c build:ts pretty build:py",
    "clean:dist": "rm -r ./dist/*",
    "clean:sandbox": "rm -r ./sandbox/*"
  },
  "dependencies": {}
}
